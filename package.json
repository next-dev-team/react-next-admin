{
  "name": "react-admin-pro",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "================DEV================": "=======",
    "================ENV================": "=======",
    "================TOOLS================": "=======",
    "==============Build/Demo==============": "=======",
    "build": "pnpm env:prod max build",
    "build:dev": "pnpm env:dev max build",
    "build:pre": "pnpm env:pre max build",
    "codegen": "DOTENV_CONFIG_PATH=./.env graphql-codegen -r dotenv/config --config codegen.yml",
    "codegen:watch": "yarn codegen -- --watch",
    "dev": "pnpm env:dev max dev",
    "dev:pre": "pnpm env:pre max dev",
    "e2e": "cypress run",
    "e2e:ci": "start-server-and-test preview http://127.0.0.1:9572  e2e",
    "env:dev": "cross-env UMI_ENV=dev",
    "env:pre": "cross-env UMI_ENV=pre",
    "env:prod": "cross-env UMI_ENV=prod",
    "init": "npx vh-cli-git-commit && npx vh-cli-lint-init",
    "postinstall": "max setup",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "open-api": "vh-cli-openapi -p http://xxx.xxx.xxx/v3/api-docs",
    "prepare": "husky install",
    "prettier:fix": "prettier --config .prettierrc.js --write 'src/**/*.{js,jsx,tsx,ts,less,scss,md,json}'",
    "preview": "max preview",
    "test": "jest",
    "umi:clean": "rm -rf node_modules/.cache && src/.umi",
    "umig": "max g"
  },
  "lint-staged": {
    "*.less": "stylelint --syntax less",
    "*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "*.{js,jsx,less,scss,md,json}": [
      "prettier  --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript  --write"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.4.2",
    "@ant-design/pro-components": "^2.3.28",
    "@apollo/client": "^3.7.1",
    "ahooks": "^3.7.2",
    "antd": "^4.24.1",
    "antd-img-crop": "^4.2.8",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.6",
    "eslint-plugin-valtio": "^0.6.1",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "lodash-es": "^4.17.21",
    "next-dev-antd-ui": "^0.0.14-beta.1",
    "next-dev-utils": "^1.0.11-rc1",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.11",
    "@graphql-codegen/import-types-preset": "^2.2.6",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/plugin-helpers": "^2.7.2",
    "@graphql-codegen/typescript": "^2.8.1",
    "@graphql-codegen/typescript-operations": "^2.5.6",
    "@graphql-codegen/typescript-react-apollo": "^3.3.6",
    "@iconify/json": "^2.1.133",
    "@milahu/patch-package": "^6.4.14",
    "@svgr/core": "^6.5.1",
    "@testing-library/react": "^13.4.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^29.2.2",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@umijs/codemod": "^0.1.0",
    "@umijs/max": "^4.0.29",
    "@vh-cli/git-commit": "^1.0.7",
    "@vh-cli/lint": "^0.0.14",
    "carlo": "^0.9.46",
    "commander": "^9.4.1",
    "cross-env": "^7.0.3",
    "cypress": "^10.11.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.26.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "jest": "^29.2.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react-dom": "^18.2.0",
    "react-router": "^6.4.3",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.14.1",
    "tailwindcss": "^3.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-icons": "^0.14.13"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
