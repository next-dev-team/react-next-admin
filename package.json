{
  "name": "react-maxjs-admin",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env UMI_ENV=local max dev",
    "dev": "pnpm start",
    "build:dev": "cross-env UMI_ENV=build_dev max build",
    "build:test": "cross-env UMI_ENV=build_test max build",
    "build": "cross-env UMI_ENV=build_pro max build",
    "postinstall": "max setup",
    "umig": "max g",
    "serve:dev": "serve ./dist/pro",
    "preview": "cross-env UMI_ENV=dev max preview",
    "test": "jest",
    "e2e": "cypress run",
    "e2e:ci": "start-server-and-test preview http://127.0.0.1:9572  e2e",
    "init": "npx vh-cli-git-commit && npx vh-cli-lint-init",
    "open-api": "vh-cli-openapi -p http://39.108.122.223:8060/v3/api-docs",
    "prettier:fix": "prettier --config .prettierrc.js --write 'src/**/*.{js,jsx,tsx,ts,less,scss,md,json}'",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.less": "stylelint --syntax less",
    "*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "*.{js,jsx,less,scss,md,json}": [
      "prettier  --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript  --write"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.4.2",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-components": "^2.3.13",
    "ahooks": "^3.7.1",
    "antd": "^4.23.3",
    "antd-img-crop": "^4.2.5",
    "braft-editor": "^2.3.9",
    "classnames": "^2.3.2",
    "crypto-js": "^4.1.1",
    "react": "^18.2.0",
    "react-amap": "^1.2.8",
    "react-dom": "^18.2.0",
    "react-router": "^6.4.1"
  },
  "devDependencies": {
    "@iconify/json": "latest",
    "@svgr/core": "^6.3.1",
    "@testing-library/react": "^13.4.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^29.0.3",
    "@types/mockjs": "^1.0.7",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@umijs/codemod": "^0.1.0",
    "@umijs/max": "latest",
    "@vh-cli/git-commit": "^1.0.6",
    "@vh-cli/lint": "^0.0.13",
    "@vh-cli/openapi": "^0.0.5",
    "carlo": "^0.9.46",
    "commander": "^9.4.0",
    "cross-env": "^7.0.3",
    "cypress": "^10.9.0",
    "eslint": "^8.24.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "jest": "^29.1.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "serve": "^14.0.1",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.13.0",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-icons": "^0.14.10"
  },
  "resolutions": {},
  "engines": {
    "node": ">=16.0.0"
  }
}