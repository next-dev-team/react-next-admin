{
  "name": "react-admin-pro",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "cross-env UMI_ENV=dev max dev",
    "dev:prod": "cross-env UMI_ENV=prod max dev",
    "dev:pre": "cross-env UMI_ENV=pre max dev",
    "build:dev": "cross-env UMI_ENV=dev max build",
    "build:pre": "cross-env UMI_ENV=pre max build",
    "build": "pnpm build:pre",
    "postinstall": "max setup",
    "umig": "max g",
    "preview": "cross-env UMI_ENV=pre max preview",
    "test": "jest",
    "e2e": "cypress run",
    "e2e:ci": "start-server-and-test preview http://127.0.0.1:9572  e2e",
    "init": "npx vh-cli-git-commit && npx vh-cli-lint-init",
    "prettier:fix": "prettier --config .prettierrc.js --write 'src/**/*.{js,jsx,tsx,ts,less,scss,md,json}'",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prepare": "husky install",
    "umi:clean": "rm -rf node_modules/.cache && src/.umi"
  },
  "lint-staged": {
    "*.less": "stylelint --syntax less",
    "*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "*.{js,jsx,less,scss,md,json}": [
      "prettier  --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript  --write"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "latest",
    "@ant-design/pro-components": "latest",
    "ahooks": "^3.7.1",
    "antd": "latest",
    "antd-img-crop": "^4.2.5",
    "crypto-js": "^4.1.1",
    "hox": "^2.1.0",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "dotenv": "^16.0.3",
    "@milahu/patch-package": "^6.4.14",
    "@iconify/json": "latest",
    "@svgr/core": "^6.3.1",
    "@testing-library/react": "^13.4.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^29.0.3",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@umijs/codemod": "^0.1.0",
    "@umijs/max": "latest",
    "@vh-cli/git-commit": "^1.0.6",
    "@vh-cli/lint": "^0.0.13",
    "carlo": "^0.9.46",
    "commander": "^9.4.0",
    "cross-env": "^7.0.3",
    "cypress": "^10.9.0",
    "eslint": "^8.24.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "jest": "^29.1.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react-dom": "^18.2.0",
    "react-router": "^6.4.1",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.13.0",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-icons": "^0.14.11"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}